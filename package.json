{
  "name": "remark-emoji-to-gemoji",
  "version": "0.1.0",
  "description": "Transform gemoji shortcodes into emoji unicodes",
  "license": "MIT",
  "keywords": [
    "emoji",
    "gemoji",
    "to"
  ],
  "dependencies": {
    "gemoji": "^1.0.0",
    "unist-util-visit": "^1.0.0"
  },
  "repository": "jackycute/remark-emoji-to-gemoji",
  "author": "jackycute",
  "files": [
    "index.js"
  ],
  "devDependencies": {
    "browserify": "^12.0.0",
    "eslint": "^1.0.0",
    "esmangle": "^1.0.0",
    "istanbul": "^0.4.0",
    "jscs": "^2.0.0",
    "jscs-jsdoc": "^1.0.0",
    "remark": "^3.0.0",
    "remark-comment-config": "^2.0.0",
    "remark-github": "^2.0.0",
    "remark-lint": "^2.0.0",
    "remark-slug": "^3.0.0",
    "remark-usage": "^2.0.0",
    "remark-validate-links": "^2.0.0",
    "tape": "^4.0.0"
  },
  "scripts": {
    "build-md": "remark . --quiet --frail",
    "build-bundle": "browserify index.js --no-builtins -s emojiToGemoji > remark-emoji-to-gemoji.js",
    "build-mangle": "esmangle remark-emoji-to-gemoji.js > remark-emoji-to-gemoji.min.js",
    "build": "npm run build-md && npm run build-bundle && npm run build-mangle",
    "lint-api": "eslint .",
    "lint-style": "jscs --reporter inline .",
    "lint": "npm run lint-api && npm run lint-style",
    "test-api": "node test.js",
    "test-coverage": "istanbul cover test.js",
    "test": "npm run build && npm run lint && npm run test-coverage"
  }
}
